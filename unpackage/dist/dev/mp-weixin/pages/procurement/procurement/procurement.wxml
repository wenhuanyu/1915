<view class="background"><view class="container search"><view class="flex"><uni-easyinput bind:input="__e" class="input" vue-id="a063b6c6-1" placeholder="请输入仓库名称" modelValue="{{search_name}}" data-event-opts="{{[['^input',[['__set_model',['','search_name','$event',[]]]]]]}}" bind:__l="__l"></uni-easyinput><button data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="btn-search" bindtap="__e">搜索</button></view><view class="flex mt-20"><view class="text">自定义查</view><uni-datetime-picker bind:input="__e" class="date-picker vue-ref" vue-id="a063b6c6-2" type="date" placeholder="开始时间" data-ref="formDate" modelValue="{{start_time}}" data-event-opts="{{[['^input',[['__set_model',['','start_time','$event',[]]]]]]}}" bind:__l="__l"></uni-datetime-picker>至<uni-datetime-picker bind:input="__e" class="date-picker vue-ref" vue-id="a063b6c6-3" type="date" placeholder="结束时间" data-ref="formDate" modelValue="{{end_time}}" data-event-opts="{{[['^input',[['__set_model',['','end_time','$event',[]]]]]]}}" bind:__l="__l"></uni-datetime-picker><button data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="btn-search btn-color" bindtap="__e">查询</button></view></view><view class="container list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="item"><view class="big ellipsis">{{'采购单号'+item.no+''}}</view><view class="flex top .mb-10"><view class="title grey">采购数量</view><view class="value">{{item.total_count}}</view></view><view class="flex .mb-10"><view class="title grey">供应商</view><view class="value">{{item.apply_name}}</view></view><view class="flex .mb-10"><view class="title grey">采购仓库</view><view class="value">{{item.ware_name}}</view></view><view class="flex .mb-10"><view class="title grey">采购员</view><view class="value">{{item.business_people}}</view></view><block wx:if="{{item.check_user}}"><view class="flex .mb-10"><view class="title grey">审核员</view><view class="value">{{item.check_user}}</view></view></block><view class="flex .mb-10"><view class="title grey">时间</view><view class="value">{{item.create_time}}</view></view><view class="flex .mb-10 last"><view class="title grey">备注</view><view class="value">{{item.remark}}</view></view><view data-event-opts="{{[['tap',[['detail',['$0'],[[['list','id',item.id,'id']]]]]]]}}" class="arrow flex flex-column" bindtap="__e"><view>详情</view></view><block wx:if="{{item.check_status==0}}"><view class="rightbox rightboxBack">{{''+item.check_status_text+''}}</view></block><block wx:if="{{item.check_status==1}}"><view class="rightbox jujueBack">{{''+item.check_status_text+''}}</view></block><block wx:if="{{item.check_status==2}}"><view class="rightbox shibaieBack">{{''+item.check_status_text+''}}</view></block><block wx:if="{{item.check_status==3}}"><view class="rightbox dairukuBack">{{''+item.check_status_text+''}}</view></block><block wx:if="{{item.check_status==4}}"><view class="rightbox shenheBack">{{''+item.check_status_text+''}}</view></block></view></block></view></view>