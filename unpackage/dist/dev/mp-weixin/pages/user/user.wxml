<view class="container"><block wx:if="{{isLogin}}"><block><view class="flex"><image class="avatar" src="{{avatar}}"></image><view class="info"><view class="nickname">{{''+nickname+''}}</view><view class="tel">{{''+tel+''}}</view></view></view><view class="timeBox"><view>剩余使用时间</view><view class="time_day"><view class="day">{{expire_in}}</view><view>天</view></view></view><view class="personal_list"><block wx:for="{{personal_list}}" wx:for-item="item" wx:for-index="__i0__" wx:key="title"><block><view data-event-opts="{{[['tap',[['jump',['$0'],[[['personal_list','title',item.title,'id']]]]]]]}}" class="item" bindtap="__e"><view class="icon_text"><view><image src="{{item.icon}}"></image></view><view class="icon_title">{{item.title}}</view></view></view></block></block><view class="item"><button class="button" open-type="contact"><view class="icon_text"><view><image src="/static/image/icon6.png"></image></view><view class="icon_title">联系客服</view></view></button></view><block wx:for="{{personal_list_my}}" wx:for-item="item" wx:for-index="__i1__" wx:key="name"><block><navigator class="item" url="{{item.url}}"><view class="icon_text"><view><image src="{{item.image}}"></image></view><view class="icon_title">{{item.name}}</view></view></navigator></block></block></view><view data-event-opts="{{[['tap',[['tiaozhuan',['$event']]]]]}}" class="bottom_text" bindtap="__e">技术支持：木子天禾科技</view></block></block><block wx:else><block><view class="flex "><image class="avatar" src="/static/image/user-default.png"></image><view data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="login" bindtap="__e">点击登录</view></view></block></block></view>