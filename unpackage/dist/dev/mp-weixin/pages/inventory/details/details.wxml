<view class="background"><view class="details_list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="__i0__"><view class="details_item flex_between"><view><view class="details_item_list"><view class="title">商品条码</view><view class="value">{{item.bar_code}}</view></view><view class="details_item_list"><view class="title">商品名称</view><view class="value">{{item.good_name}}</view></view><view class="details_item_list"><view class="title">商品分类</view><view class="value">{{item.cate_name}}</view></view><view class="details_item_list"><view class="title">盘前库存</view><view class="value">{{item.num}}</view></view><view class="details_item_list"><view class="title">盘点库存</view><view class="value">{{item.check_num}}</view></view><view class="details_item_list"><view class="title">盘点人</view><view class="value">{{item.user_name}}</view></view><view class="details_item_list"><view class="title">仓库名</view><view class="value">{{item.ware_name}}</view></view><view class="details_item_list"><block wx:if="{{item.type==1}}"><view class="title">盘盈</view></block><block wx:if="{{item.type==2}}"><view class="title" style="color:#F96164;">盘亏</view></block><block wx:if="{{item.type==1}}"><view class="value">{{item.nums}}</view></block><block wx:if="{{item.type==2}}"><view class="value" style="color:#F96164;">{{item.nums}}</view></block></view></view><block wx:if="{{item.image}}"><image class="image" src="{{item.image}}"></image></block><block wx:else><image class="image" src="/static/image/default.png"></image></block></view></block></view></view>