<view class="fix-padding"><view class="container"><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="itemList mb-20"><view class="itemList_top"><view class="mr-20">商品分类名称</view><view>{{item.name}}</view></view><view class="flex"><button data-event-opts="{{[['tap',[['editor',['$0'],[[['list','id',item.id]]]]]]]}}" class="button button_color mr-20" bindtap="__e">编辑</button><button data-event-opts="{{[['tap',[['del',['$0'],[[['list','id',item.id]]]]]]]}}" class="button mr-20" bindtap="__e">删除</button><block wx:if="{{item.status==1}}"><button data-event-opts="{{[['tap',[['on_off',['$0'],[[['list','id',item.id]]]]]]]}}" class="button button_color_last" bindtap="__e">停用</button></block><block wx:else><button data-event-opts="{{[['tap',[['on_off',['$0'],[[['list','id',item.id]]]]]]]}}" class="button button_color_last" style="background:#007AFF;" bindtap="__e">启用</button></block></view></view></block></radio-group></view><view class="container fix-bottom"><button data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btn-submit" bindtap="__e">添加</button></view></view>