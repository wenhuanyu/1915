<view class="background"><view class="details_list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="__i0__"><view class="details_item flex_between"><view><view class="details_item_list"><view class="title">商品名称</view><view class="value">{{item.goods_name}}</view></view><view class="details_item_list"><view class="title">采购数量</view><view class="value">{{item.count}}</view></view><view class="details_item_list"><view class="title">采购仓库</view><view class="value">{{item.warehouse_name}}</view></view><view class="details_item_list"><view class="title">供应商名</view><view class="value">{{item.apply_name}}</view></view><view class="details_item_list"><view class="title">采购员名</view><view class="value">{{item.business_people}}</view></view><block wx:if="{{check_user}}"><view class="details_item_list"><view class="title">审核员名</view><view class="value">{{check_user}}</view></view></block><view class="details_item_list"><view class="title">备注内容</view><view class="value">{{item.remark}}</view></view></view><block wx:if="{{item.image}}"><image class="image" src="{{item.image}}"></image></block><block wx:else><image class="image" src="/static/image/default.png"></image></block></view></block></view><view class="details_note"><view class="title">采购备注</view><view class="value">{{remark}}</view></view><block wx:if="{{list[0].check_status==0}}"><view class="container fix-bottom flex"><button data-event-opts="{{[['tap',[['submit_shenhe',['$event']]]]]}}" class="btn-submit" style="width:100%;" bindtap="__e">审核通过</button><button data-event-opts="{{[['tap',[['submit_jujue',['$event']]]]]}}" class="btn-submit" style="width:100%;background:#F96164;margin-left:20rpx;" bindtap="__e">审核拒绝</button></view></block></view>