<view class="background fix-padding"><view class="details_list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="__i0__"><view class="details_item flex_between"><view><view class="details_item_list"><view class="title">商品名称</view><view class="value">{{item.goods_name}}</view></view><view class="details_item_list"><view class="title">采购数量</view><view class="value">{{item.count}}</view></view><view class="details_item_list"><view class="title">采购仓库</view><view class="value">{{item.warehouse_name}}</view></view><view class="details_item_list"><view class="title">供应商名</view><view class="value">{{item.apply_name}}</view></view><view class="details_item_list"><view class="title">采购员名</view><view class="value">{{item.business_people}}</view></view><block wx:if="{{check_user}}"><view class="details_item_list"><view class="title">审核员名</view><view class="value">{{check_user}}</view></view></block><view class="details_item_list"><view class="title">备注内容</view><view class="value">{{item.remark}}</view></view></view><block wx:if="{{item.image}}"><image class="image" src="{{item.image}}"></image></block><block wx:else><image class="image" src="/static/image/default.png"></image></block></view></block></view><view class="details_note"><view class="title">采购备注</view><view class="value">{{remark}}</view></view><block wx:if="{{list[0].check_status==3}}"><view class="container fix-bottom"><button data-event-opts="{{[['tap',[['submit_shenhe',['$event']]]]]}}" class="btn-submit" bindtap="__e">入库</button></view></block></view>